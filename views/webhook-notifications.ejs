<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Events</title>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/10.7.1/styles/default.min.css">
</head>
<body>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/10.7.1/highlight.min.js"></script>
  <script src="events.js"></script>
  <script>
    let events = '<%-events%>';
    events = events.replaceAll('}{','}\n{');
    myArr = events.split('\n');
    for(i=(myArr.length-1);i>=0;i--){
      const code_elem = document.createElement('code');
      const event = JSON.parse(myArr[i]);
      code_elem.innerText = JSON.stringify(event, null, 2);
      const pre_elem = document.createElement('pre');
      pre_elem.appendChild(code_elem);
      document.body.appendChild(pre_elem);
    }
    hljs.highlightAll();
  </script>
</body>
</html>